{
  "name": "site-pessoal-react",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://marcostavares.dev.br",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "node scripts/updateRoutes.js",
    "build": "vite build && npx react-snap",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@mui/icons-material": "^6.1.4",
    "@mui/material": "^6.1.4",
    "@samkirkland/ftp-deploy": "^1.2.4",
    "axios": "^1.11.0",
    "formik": "^2.4.6",
    "framer-motion": "^12.4.0",
    "next": "^15.4.7",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.7.0",
    "styled-components": "^6.1.13",
    "typescript": "^5.4.5",
    "web-vitals": "^2.1.4",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@types/node": "^22.12.0",
    "@types/react": "^19.0.35",
    "@types/react-dom": "^19.0.17",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@vitejs/plugin-legacy": "^7.2.1",
    "@vitejs/plugin-react": "^5.0.0",
    "react-snap": "^1.23.0",
    "vite": "^7.1.12"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "reactSnap": {
    "inlineCss": true,
    "crawl": true,
    "source": "dist",
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "minifyHtml": true,
    "skipThirdPartyRequests": true,
    "include": [
      "/",
      "/blog",
      "/blog/category/node",
      "/blog/category/react",
      "/blog/category/javascript",
      "/blog/category/php"
    ]
  }
}